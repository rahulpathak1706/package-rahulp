{
    "name": "rahulp/crud-generator",
    "description": "A Laravel package for generating CRUD operations with Repository Pattern",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rahulp\\CrudGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "rahulp",
            "email": "rahulspathak17@gmail.com",
            "homepage": "https://github.com/rahulpathak1706"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rahulp\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
